    "koa-logger": "^2.0.1",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-session": "^5.0.0",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "vue": "^2.3.3",
